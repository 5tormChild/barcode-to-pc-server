  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build": "ng build",
    "preview": "npm rebuild node-sass && ng build --output-path=electron/app && cd electron && npm run preview",
    "dist": "ng build --prod --output-path=electron/app && cd electron && npm run dist",
    "win-dist": "ng build --prod --output-path=electron/app && cd electron && npm run win-dist",    
    "postinstall": "cd electron && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
