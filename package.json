    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6",
    "bootstrap-sass": "^3.3.7",
    "file-saver": "^1.3.3",
    "ng2-bootstrap": "^1.1.16-11",
    "ngx-clipboard": "^3.0.7",
    "papaparse": "^4.1.2"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.2",
    "@angular/compiler-cli": "^2.4.0",
